{
    "name": "lutfi-nur/query-builder",
    "description": "Fluent datatable query builder for Laravel with Yajra Datatables. Supports MySQL and PostgreSQL.",
    "keywords": ["laravel", "datatable", "yajra", "query-builder", "mysql", "postgresql"],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Lutfi Nur",
            "email": "iseplutpinur7@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "yajra/laravel-datatables-oracle": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "LutfiNur\\QueryBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LutfiNur\\QueryBuilder\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
